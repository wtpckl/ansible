GNUPGHOME	OVERRIDE=@{HOME}/.config/gnupg
NVM_DIR 	OVERRIDE=@{HOME}/.config/nvm
PARALLEL_HOME	OVERRIDE=@{HOME}/.config/parallel
PYENV_ROOT	OVERRIDE=@{HOME}/.config/pyenv
ZDOTDIR		OVERRIDE=@{HOME}/.config/zsh
TEXMFHOME	OVERRIDE=@{HOME}/.local/share/texmf
CARGO_HOME	OVERRIDE=@{HOME}/.cache/cargo
RUSTUP_HOME	OVERRIDE=@{HOME}/.config/rustup

PATH		OVERRIDE="${PYENV_ROOT/bin}:${PATH}"

npm_config_devdir	OVERRIDE=@{HOME}/.cache/node-gyp
